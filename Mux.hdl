/** 
 * Multiplexor. If sel==1 then out=b else out=a.
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel, out=notsel);
    And(a=a, b=notsel, out=top);
    And(a=b, b=sel, out=bottom);
    Or(a=top, b=bottom, out=out);
}