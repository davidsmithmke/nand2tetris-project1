/**
 *  Exclusive-or gate: out = !(a == b).
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=b, out=nandab);
    Nand(a=a, b=nandab, out=top);
    Nand(a=b, b=nandab, out=bottom);
    Nand(a=top, b=bottom, out=out);
}